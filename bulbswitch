class Solution {
public:
    int bulbSwitch(int n) {
        bool bulbs[n];
        int number=0;
        for(int i=1;i<n+1;i++)
        {
            for(int j=1;j<n+1;j++)
            {
                if(j%i==0)
                {
                    bulbs[j-1]=!bulbs[j-1];
                }
            }
        }
        for(int i=0;i<n;i++)
        {
            if(bulbs[i]==true)
              number++;
        }
        return number;
        
    }
};
